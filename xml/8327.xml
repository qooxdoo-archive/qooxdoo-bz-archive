            <commentid>40840</commentid>
            <who name="Martin Wittemann">martin.wittemann</who>
            <bug_when>2014-07-28 13:06:39 +0200</bug_when>
            <thetext>Here is the reduced sample file I used for debugging:

qx.Class.define(&quot;test.Application&quot;,
{
  extend : qx.application.Standalone,

  members : {
    main: function() {
      this.base(arguments);

      var outerScroll = new qx.ui.container.Scroll().set({
        width: 350,
        height: 250,
        backgroundColor: &quot;green&quot;
      });

      var innerScroll = new qx.ui.container.Scroll().set({
        maxWidth: 270,
        minHeight: 300
      });

      innerScroll.add(this.generateBoxes());
      outerScroll.add(innerScroll);

      this.getRoot().add(outerScroll, {left: 10, top: 50});
    },

    generateBoxes : function() {
      var box = new qx.ui.container.Composite(new qx.ui.layout.Grid());

      for (var y=0; y&lt;20; y++) {
        for (var x=0; x&lt;5; x++) {
          box.add((new qx.ui.core.Widget()).set({
            backgroundColor : ((x+y) % 2 == 0) ? &quot;red&quot; : &quot;blue&quot;,
            width : 50,
            height : 50,
            draggable : true
          }), {column: x, row: y});
        }
      }

      return box;
    }
  }
});</thetext>
          </long_desc>
